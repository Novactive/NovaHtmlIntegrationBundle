{
  "name": "novactive/ezstatictemplatesbundle",
  "type": "symfony-bundle",
  "description": "This bundle is used to expose twig templates to a siteaccess",
  "keywords": [
    "novactive",
    "static template"
  ],

  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Novactive",
      "homepage": "http://www.novactive.com",
      "email": "dir.tech@novactive.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ezsystems/ezpublish-kernel": "^7.0"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^4.0",
    "friendsofphp/php-cs-fixer": "^2.10",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Novactive\\Bundle\\EzStaticTemplatesBundle\\": "bundle"
    }
  }
